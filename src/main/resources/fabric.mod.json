{
  "schemaVersion": 1,
  "id": "enderbench",
  "version": "${version}",

  "name": "Ender Bench",
  "description": "Ender Bench is a small Minecraft mod for Fabric that adds a new block to the game. A vanilla style way to remotely access your storage",
  "authors": [
    "legoatoom"
  ],
  "contact": {
    "homepage": "https://github.com/legoatoom/EnderBench",
    "sources": "https://github.com/legoatoom/EnderBench"
  },

  "license": "GPL-3.0",
  "icon": "assets/enderbench/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "com.github.legoatoom.enderbench.EnderBench"
    ],
    "client": [
      "com.github.legoatoom.enderbench.client.EnderBenchClient"
    ]
  },
  "mixins": [
    "enderbench.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.7.4",
    "fabric": "*",
    "minecraft": "1.16.x"
  },
  "suggests": {
    "flamingo": "*"
  }
}
